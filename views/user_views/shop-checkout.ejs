    <script src="https://checkout.razorpay.com/v1/checkout.js"></script>
    <script>
            //function to copy code to clipboard
        function copyToClipboard(code) {
            navigator.clipboard.writeText(code).then(()=> {
                Swal.fire({
                position: 'center',
                icon: 'success',
                title: "Coupon Code copied to Clipboard",
                showConfirmButton: false,
                timer: 1500
                });
            }).catch(err=>{
                console.error('Unable to copy to clipboard', error);
            });
        }
        
        document.addEventListener("DOMContentLoaded", ()=>{
        const applyButton = document.getElementById('apply-button');
        const couponInput = document.getElementById('coupon-input');
        const couponDiscount = document.querySelector('.coupon-discount');
        const couponDiscountTR = document.querySelector('.coupon-discount-tr');
        const grandTotalValue = document.querySelector('.grand-total').textContent.trim();
        const grandTotal = document.querySelector('.grand-total');
        const removeButton = document.getElementById('remove-button');
        let couponCode = null;
        removeButton.style.display = 'none';
        
        applyButton.addEventListener("click", function (e) {
            e.preventDefault();
            if(validateCode()){
                fetch(`/apply-coupon?couponCode=${couponInput.value}&grandTotal=${grandTotalValue}`, {
                method: 'GET',
                headers: {
                    'Content-Type': 'application/json'
                }
                })
                .then(response=>{
                    if(!response.ok){
                        throw new Error(`HTTP Error! status:${response.status}`);
                    }
                    return response.json();
                })
                .then(data=>{
                    if(data.error){
                        Swal.fire({
                        position: 'center',
                        icon: 'error',
                        title: data.error,
                        showConfirmButton: false,
                        timer: 1500
                        });
                    }else if(data.message){
                        Swal.fire({
                        position: 'center',
                        icon: 'success',
                        title: data.message,
                        showConfirmButton: false,
                        timer: 1500
                        });
                        originalGrandTotal = data.grandTotal;
                        grandTotal.innerText = data.updatedTotal;
                        couponDiscount.innerText = data.couponDiscount;
                        applyButton.style.display = 'none';
                        removeButton.style.display = 'inline';
                        couponDiscountTR.classList.remove('hidden');
                        couponInput.readOnly = true;
                        couponCode = couponInput.value;
                    }
                }).catch(error=>{
                    window.location.href = '/login'
                    console.error("Error:", error);
                })
            
            }
        });
        removeButton.addEventListener("click", ()=>{
            couponInput.value = "";
            removeButton.style.display = 'none';
            applyButton.style.display = 'inline';
            couponDiscountTR.classList.add('hidden');
            couponInput.readOnly = false;
            grandTotal.innerText = originalGrandTotal;
            couponDiscount.innerText = 0;
            couponCode = null;
        })

        function validateCode() {
            let inputCouponCode = couponInput.value; // Get the current value and trim whitespaces
            let isValid = true;
            const errorElement = document.getElementById('couponError');
            errorElement.textContent = ''; // Clear previous error messages
            errorElement.style.color = '';

            if (/\s/.test(inputCouponCode)) {
                errorElement.textContent = 'Coupon cannot have whitespaces';
                errorElement.style.color = 'red';
                isValid = false;
            }

            if (inputCouponCode === '') {
                errorElement.textContent = 'Coupon cannot be empty';
                errorElement.style.color = 'red';
                isValid = false;
            }

            return isValid;
        }


        const codOrderButton = document.getElementById('cod-orderButton');
        codOrderButton.addEventListener("click", (e)=>{
            e.preventDefault();
            const radioButtons = document.querySelectorAll('.select-address');
            let selectedAddressValue = null;
            for (const radioButton of radioButtons) {
                
                if(radioButton.checked){
                    selectedAddressValue = radioButton.value;
                    fetch(`place-order-cod/${selectedAddressValue}?couponCode=${couponCode || ''}`)
                    .then(response=>{
                        if(!response.ok){
                            throw new Error(`HTTP Error! status:${response.status}`)
                        }
                        return response.json();
                    })
                    .then(data=>{
                        if(data.backToCart){
                            window.location.href = '/cart';
                        }
                        if(data.error){
                            Swal.fire({
                            position: 'center',
                            icon: 'error',
                            title: data.error,
                            showConfirmButton: false,
                            timer: 1500
                            });
                        }else if(data.orderId){
                            window.location.href = `/order-placed/${data.orderId}`
                        }
                    }).catch(error=>{
                        window.location.href = '/login';
                        console.error(error);
                    })
                    break;
                }
            }

            if (selectedAddressValue !== null) {
            console.log("Selected Address ID:", selectedAddressValue);
            } else {
            Swal.fire({
                icon: 'error',
                title: 'Oops...',
                text: 'No Address is Selected!',
            })
            }
        });


        const razorpayButton = document.getElementById('rzp-orderButton');
        razorpayButton.addEventListener("click", (e)=>{
            e.preventDefault();
            const radioButtons = document.querySelectorAll('.select-address');
            let selectedAddressValue = null;
            for (const radioButton of radioButtons) {
                
                if(radioButton.checked){
                    selectedAddressValue = radioButton.value;
                    fetch(`place-order-razorpay/${selectedAddressValue}?couponCode=${couponCode || ''}`)
                    .then(response=>{
                        if(!response.ok){
                            throw new Error(`HTTP Error! status:${response.status}`)
                        }
                        return response.json();
                    })
                    .then(data=>{
                        if(data.backToCart){
                            window.location.href = '/cart';
                        }
                        if(data.error){
                            Swal.fire({
                            position: 'center',
                            icon: 'error',
                            title: data.error,
                            showConfirmButton: false,
                            timer: 1500
                            });
                        }else if(data.razorOrderId){
                            var razorOrderId = data.razorOrderId;
                            var orderId = data.orderId
                            // Initialize the Razorpay payment with the order_id
                            var options = {
                            key: "rzp_test_gBU9v2ubdClFXs",
                            order_id: razorOrderId,
                            handler: (response)=>{
                                console.log(response);
                                //payment successfull
                                handlePaymentSuccess(razorOrderId, response, orderId)
                            }
                            };
                            var rzp1 = new Razorpay(options);
                            rzp1.open();
                            rzp1.on('payment.failed', response=>{
                                console.log("Payment Failed");
                                handlePaymentFailure(razorOrderId, response, orderId);
                            });
                        }
                    }).catch(error=>{
                        console.error(error);
                    })
                    break;
                }
            }

            if (selectedAddressValue !== null) {
            console.log("Selected Address ID:", selectedAddressValue);
            } else {
            Swal.fire({
                icon: 'error',
                title: 'Oops...',
                text: 'No Address is Selected!',
            })
            }
        });

        //function to handle payment success
        function handlePaymentSuccess(razorOrderId, response, orderId) {
            updatePaymentStatus("Success", razorOrderId, response, orderId);
        }

        //function to handle payment failure
        function handlePaymentFailure(razorOrderId, response, orderId) {
            updatePaymentStatus("Failed", razorOrderId, response, orderId)
        }

        // Function to update the payment status in the database
        function updatePaymentStatus(paymentStatus, razorOrderId, response, orderId) {
            const url = `/update-payment-status?paymentStatus=${paymentStatus}&razorOrderId=${razorOrderId}&orderId=${orderId}&couponCode=${couponCode || ''}`

            fetch(url, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
            })
            .then(response=>{
                if(!response.ok){
                    throw new Error('Failed to update payment status');
                }
                return response.json();
            })
            .then(data=>{
                if(data.paymentStatus == "Success"){
                    window.location.href = `/order-placed/${orderId}`
                }else{
                    window.location.href = '/profile'
                }
            })
            .catch(error=>{
                console.error('Failed to update payment status', error);
                window.location.href = '/login';
            })
        }


        const walletOrderButton = document.getElementById('wallet-orderButton');
        walletOrderButton.addEventListener("click", (e)=>{
            e.preventDefault();
            const radioButtons = document.querySelectorAll('.select-address');
            let selectedAddressValue = null;
            for (const radioButton of radioButtons) {
                
                if(radioButton.checked){
                    selectedAddressValue = radioButton.value;
                    fetch(`place-order-wallet/${selectedAddressValue}?couponCode=${couponCode || ''}`)
                    .then(response=>{
                        if(!response.ok){
                            throw new Error(`HTTP Error! status:${response.status}`)
                        }
                        return response.json();
                    })
                    .then(data=>{
                        if(data.backToCart){
                            window.location.href = '/cart';
                        }
                        if(data.error){
                            Swal.fire({
                            position: 'center',
                            icon: 'error',
                            title: data.error,
                            showConfirmButton: false,
                            timer: 1500
                            });
                        }else if(data.orderId){
                            window.location.href = `/order-placed/${data.orderId}`
                        }
                    }).catch(error=>{
                        window.location.href = '/login';
                        console.error(error);
                    })
                    break;
                }
            }

            if (selectedAddressValue !== null) {
            console.log("Selected Address ID:", selectedAddressValue);
            } else {
            Swal.fire({
                icon: 'error',
                title: 'Oops...',
                text: 'No Address is Selected!',
            })
            }
        });


        });


        </script>

