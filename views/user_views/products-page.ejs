<body>
    <header class="header-area header-style-3 header-height-2">
        <div class="header-top header-top-ptb-1 d-none d-lg-block">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-xl-3 col-lg-4">
                        <div class="header-info">
                            <ul>
                                <li><i class="fi-rs-smartphone"></i> <a href="#">(+01) - 2345 - 6789</a></li>
                                <li><i class="fi-rs-marker"></i><a  href="page-contact.html">Our location</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-xl-6 col-lg-4">
                        <div class="text-center">
                            <div id="news-flash" class="d-inline-block">
                                <ul>
                                    <li>Get great devices up to 50% off <a href="/products"> | View details</a></li>
                                    <li>Supper Value Deals - Save more with coupons</li>
                                    <li>Aesthetic Headphones, save up 35% off today <a href="/products">Shop now</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-4">
                        <div class="header-info header-info-right">
                            <ul>
                                <li>
                                    <a class="language-dropdown-active" href="#"> <i class="fi-rs-world"></i> English <i class="fi-rs-angle-small-down"></i></a>
                                    <!-- <ul class="language-dropdown">
                                        <li><a href="#"><img src="/user_assets/imgs/theme/flag-fr.png" alt="">Français</a></li>
                                        <li><a href="#"><img src="/user_assets/imgs/theme/flag-dt.png" alt="">Deutsch</a></li>
                                        <li><a href="#"><img src="/user_assets/imgs/theme/flag-ru.png" alt="">Pусский</a></li>
                                    </ul> -->
                                </li>
                                <% if(!userSession){ %> 
                                    <li><i class="fi-rs-user"></i><a href="/login">Log In / Sign Up</a></li>
                                <% }else{ %>
                                    <div class="user-details d-flex align-items-center justify-content-evenly">
                                        <li><i class="fi-rs-user"></i><a class="mr-10" href="/logout">Logout</a></li>
                                    <li><i class="fi-rs-user"></i><a class="mr-10" href="/profile"><%= userSession.username %></a></li>
                                    <a class="profile-link" href="/profile">
                                        <div class="profile-pic mr-10" style="width: 40px; height: 40px; border-radius: 50%; overflow: hidden;">
                                            <img src="<%= user.userProfile %>" alt="User Profile Picture" style="width: 100%; height: 100%; object-fit: cover; border-radius: 50%;">
                                        </div>
                                    </a> 
                                    </div>
