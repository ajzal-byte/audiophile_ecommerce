    </header>

    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="/" rel="nofollow">Home</a>
                    <span></span> Shop
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row flex-row-reverse">
                    <div class="col-lg-9">
                        <div class="shop-product-fillter">
                            <div class="sort-by-product-area">
                            </div>
                        </div>
                        <div class="row product-grid-3 d-flex justify-content-center">
                            <% if (products.length > 0) { %>
                                <% products.forEach(product => { %>
                                    <div class="col-lg-5 col-md-4 col-12 col-sm-6">
                                        <div class="product-cart-wrap mb-30">
                                            <div class="product-img-action-wrap">
                                        <div class="product-img product-img-zoom">
                                            <a href="/productDetails/<%= product._id %>">
                                                <img class="default-img" src="<%= product.productImage[0]?.path %>" alt="<%= product.productName %>">
                                                <% if(product.productImage.length>1){ %>
                                                <img class="hover-img" src="<%= product.productImage[1]?.path %>" alt="<%= product.productName %>">
                                                <% } else { %>
                                                <img class="hover-img" src="<%= product.productImage[0]?.path %>" alt="<%= product.productName %>">
                                                <% } %>
                                            </a>
                                        </div>
                                        <div class="product-action-1">
                                            <a aria-label="Add To Wishlist" class="action-btn hover-up" href="shop-wishlist.html"><i class="fi-rs-heart"></i></a>
                                        </div>
                                    </div>
                                    <div class="product-content-wrap" style="height: 115px;">
                                        <div class="product-category">
                                            <a class="text-capitalize" href="/productDetails/<%= product._id %>"><%= product.category?.categoryName %></a>
                                        </div>
                                        <h2><a href="/productDetails/<%= product._id %>"><%= product.productName %></a></h2>
                                        
                                        <div class="product-price">
                                            <% if (product.offerStatus == 'Active' &&  product.startDate <= Date.now() && product.endDate > Date.now()) { %>
                                                <span>₹<%= product.salePrice - (product.salePrice * product.discountPercentage) / 100 %></span>
                                                <span class="old-price">₹  <%= product.salePrice%></span>
                                                <span class="save-price  font-md color3 ml-5 text-danger" style="font-size: small;"> - <%= product.discountPercentage %>% </span>
                                            <% }else{ %>
                                                <span>₹<%= product.salePrice%></span>
                                                <% } %>
                                        </div>
                                        <div class="product-action-1 show">
                                            <% if(product.stock>0){%>
                                                <a aria-label="Add To Cart" class="action-btn hover-up" data-product-id="<%= product._id %>" id="addtoCart" ><i class="fi-rs-shopping-bag-add"></i></a>
                                            <% }else{ %>
                                                <span class="badge text-white" style="background-color: rgb(247, 58, 58); border-radius: 7px;" >Out of Stock</span> 
                                                <% } %>
                                            </div>
                                    </div>
                                </div>
                            </div>
                        <% }) %>     
                        <% }else{ %>
                            <h1>No products Found</h1>
                            <% } %>
                    </div>
                        <!--pagination-->
                        <% if (pagination) { %>
                            <div class="pagination-area mt-15 mb-sm-5 mb-lg-0">
                                <nav aria-label="Page navigation">
                                    <ul class="pagination justify-content-start">
                                        <% if (products.length > 0) { %>
                                            <% for (let i = 1; i <= totalPages; i++) { %>
                                                <li class="page-item <%= i == current ? 'active' : '' %>">
                                                    <a class="page-link" href="/products?page=<%= i %>"><%= i %></a>
                                                </li>
                                                <% } %>                                          
                                                <!-- <li class="page-item"><a class="page-link" href="#"><i class="fi-rs-angle-double-small-right"></i></a></li> -->
                                                <% } %>
                                            </ul>
                                        </nav>
                                </div>
                        <% } %>
                    </div>

                    <script>
                        document.addEventListener("DOMContentLoaded", ()=>{
                            const addtoCart = document.querySelectorAll('#addtoCart');
            
                            addtoCart.forEach(add => {
                                add.addEventListener('click', (e)=>{
                                    e.preventDefault()
                                    const productId = add.getAttribute('data-product-id')
                                    fetch(`/addtoCart?productId=${productId}`,{method:'POST'} )
                                    .then((response)=>{
                                        if(!response.ok){
                                          throw new Error(`HTTP error! status: ${Response.status}`)
                                        }
                                        return response.json();
                                    })
            
                                    .then((data)=>{
                                        console.log("Response from server", data);
                                        if(data.message){
                                        Swal.fire({
                                        position: 'top-center',
                                        icon: 'success',
                                        title: data.message,
                                        showConfirmButton: false,
                                        timer: 1500
                                    });
                                        }else if (data.error){
                                            window.location.href = '/login';
                                        }
                                    }).catch(error => {
                                        window.location.href = '/login'
                                        console.error("Error:", error);
                                    });         
            
                                })
                                
                            });
                        });
                    </script>

                    <div class="col-lg-3 primary-sidebar sticky-sidebar">
                        <button class="btn btn-primary mb-2" onclick="window.location.href='/products'">Clear Filter</button>
                        <form action="/filter-product" method="get" class="row">
                        <div class="widget-category mb-30">
                            <h5 class="section-title style-1 mb-20 wow fadeIn animated">Category</h5>
                            <ul class="categories">
                                <% if (categories) { %>                                 
                                <% categories.forEach(category => { %>
                                    <li>
                                        <div class="form-check">
                                            <input class="form-check-input" type="radio" name="categoryId" id="<%= category._id %>" value="<%= category._id %>">
                                            <label class="form-check-label text-capitalize" for="<%= category._id %>"><%= category.categoryName %></label>
                                        </div>
                                    </li>
                                <% }) %>
                                <% }else{ %>
                                    <li><a href="">No Categories Found</a></li>
                                <% } %>
                            </ul>
                            
                        </div>
                        <!-- Fillter By Price -->
                        <div class="sidebar-widget price_range range mb-30">
                            <div class="widget-header position-relative mb-10 pb-10">
                                <h5 class="widget-title mb-5">Sort By</h5>
                                <div class="bt-1 border-color-1"></div>
                            </div>
                            <div class="list-group">
                                <div class="list-group-item mb-10 mt-5">
                                    <label class="fw-900">Price</label>
                                    <div class="custome-radio">
                                        <input class="form-check-input" type="radio" name="sort" id="lowToHigh" value=1>
                                        <label class="form-check-label" for="lowToHigh"><span>Low to High</span></label>
                                        <br>
                                        <input class="form-check-input" type="radio" name="sort" id="highToLow" value=-1>
                                        <label class="form-check-label" for="highToLow"><span>High to Low</span></label>
                                        <br>
                                    </div>
                                </div>
                                
                            </div>
                            <button type="submit" class="btn btn-sm btn-default"><i class="fi-rs-filter mr-5"></i>Apply Filter</button>
                        </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>
    </main>
