<table id="wallet-orders">
  <thead>
    <tr>
      <th>Date</th>
      <th>Type</th>
      <th>Amount</th>
      <th>Payment status</th>
      <th>Transaction ID</th>
    </tr>
  </thead>
  <% if(!userOrders || userOrders.length == 0 ){%>
  <tr>
    <td colspan="6">No Orders</td>
  </tr>
  <% }else {%>
  <tbody>
    <% userOrders.reverse().forEach(items => { %>
    <tr>
      <%if(items.paymentMethod === "Wallet"){%>
        <td><%= moment(items.orderDate.format("MMM Do YY")) %></td>
        <td><%=items.paymentMethod%></td>
      <td><%=items.totalAmount%></td>
      <td><%=items.paymentStatus%></td>
      <td><%=items._id%></td>
    </tr>
    <% } %>
    <% }) %>
    <% }%>
  </tbody>
</table>