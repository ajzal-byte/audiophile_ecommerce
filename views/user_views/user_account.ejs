                                    <div class="tab-pane fade" id="account-detail" role="tabpanel" aria-labelledby="account-detail-tab">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Edit Your Account Details</h5>
                                            </div>
                                            <div class="card-body">
                                                <form method="post" name="enq" id="user-form" action="/edit-profile" >
                                                    <div class="row">
                                                        <div class="form-group col-md-6">
                                                            <label>Username <span class="required">*</span></label>
                                                            <input value="<%= userDetails.username %>" required class="form-control square" name="username" id="username" type="text">
                                                            <div id="usernameError" class="error-message"></div>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Phone Number <span class="required">*</span></label>
                                                            <input value="<%= userDetails.phoneNumber %>" required class="form-control square" name="phoneNumber" id="phoneNumber" type="phoneNumber">
                                                            <div id="phoneNumberError" class="error-message"></div>
                                                        </div>
                                                        <div class="col-md-12">
                                                            <button type="submit" class="btn btn-fill-out submit" id="save-changes" name="submitBtn" value="Submit">Save Changes</button>
                                                        </div>
                                                        <div class="form-check mt-20">
                                                            <input class="form-check-input" type="checkbox" value="" id="password-form-check" >
                                                            <label class="form-check-label" for="flexCheckDefault">
                                                              Change Password?
                                                            </label>
                                                          </div>
                                                    </div>
                                                </form>
                                                <form method="post" name="enq" id="password-form" >
                                                    <div class="form-group col-md-12 mt-10">
                                                        <label>Current Password <span class="required">*</span></label>
                                                        <input required class="form-control square" name="current-password" id="current-password" type="password">
                                                        <div id="passwordError" class="error-message"></div>
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label>New Password <span class="required">*</span></label>
                                                        <input required class="form-control square" name="new-password" id="new-password" type="password">
                                                        <div id="npasswordError" class="error-message"></div>
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label>Confirm Password <span class="required">*</span></label>
                                                        <input required class="form-control square" name="confirm-password" id="confirm-password" type="password">
                                                        <div id="cpasswordError" class="error-message"></div>
                                                    </div>
                                                    <div class="col-md-12">
                                                        <button type="submit" class="btn btn-fill-out submit" name="submit" id="change-pass" value="Submit">Change Password</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                        <script>
                                            document.addEventListener("DOMContentLoaded", () => {
                                              const checkbox = document.getElementById('password-form-check');
                                              const passwordForm = document.getElementById('password-form');
                                          
                                              // Initial hiding
                                                passwordForm.style.display = "none";
                                              
                                              // display the form
                                              checkbox.addEventListener('change', () => {
                                                if (checkbox.checked) {
                                                    passwordForm.style.display = "block";
                                                } else {
                                                    passwordForm.style.display = "none";
                                                }
                                              });


                                              const saveChangesButton = document.getElementById('save-changes');
                                              saveChangesButton.addEventListener("click", (e)=>{
                                                  e.preventDefault();

                                                if(validateForm()){
                                              const userForm = document.getElementById('user-form');

                                                    document.getElementById('user-form').submit()
                                                }

                                                function validateForm(){
                                                    resetErrorMessages() 
                                                    const username = document.getElementById('username').value;
                                                    const phoneNumber = document.getElementById('phoneNumber').value;
                                                    
                                                    const usernamePattern = /^[A-Za-z][A-Za-z0-9]*$/;
                                                    if (!username.match(usernamePattern)) {
                                                        showError('usernameError', 'Username should contain only characters without whitespace and numbers after at least one character');
                                                        return false;
                                                    }

                                                    if (isNaN(phoneNumber) || phoneNumber.length !== 10 || phoneNumber[0] === '0') {
                                                        showError('phoneNumberError', 'Phone number must be a number with exactly 10 digits and should not start with 0');
                                                        return false;
                                                    }
                                                    return true;
                                                }

                                              })


                                              const changePasswordBtn = document.getElementById('change-pass');
                                              changePasswordBtn.addEventListener("click", (e)=>{
                                                  e.preventDefault()
                                                  
                                                  console.log(validateForm());
                                                  if(validateForm()){
                                                    const currentPassword = document.getElementById('current-password').value;
                                                    const newPassword = document.getElementById('new-password').value;
                                                    const confirmPassword = document.getElementById('confirm-password').value;

                                                    fetch('/change-password', {
                                                        method: 'POST', 
                                                        headers: {
                                                            'Content-Type': 'application/json',
                                                        },
                                                        body: JSON.stringify({
                                                            currentPassword,
                                                            newPassword,
                                                            confirmPassword
                                                        }),
                                                    })

                                                    .then(response=>{
                                                        if(!response.ok){
                                                            throw new Error(`HTTP error! status:${response.status}`)
                                                        }
                                                        return response.json();
                                                    })

                                                    .then(data=>{
                                                        if(data.error){
                                                            Swal.fire({
                                                            position: 'top-center',
                                                            icon: 'error',
                                                            title: data.error,
                                                            showConfirmButton: false,
                                                            timer: 1500
                                                        });
                                                        }else if(data.success){
                                                            window.location.href = '/profile'
                                                        }
                                                    })
                                                }

                                                function validateForm(){
                                                resetErrorMessages() 
                                                const newPassword = document.getElementById('new-password').value;
                                                const confirmPassword = document.getElementById('confirm-password').value;
                                                    // Validate password length and no whitespaces at the beginning
                                                if (newPassword.trim().length < 6 || newPassword !== newPassword.trim()) {
                                                    showError('npasswordError', 'Password must be at least 6 characters and should not start with whitespaces');
                                                    return false;
                                                }
                                                // Validate password confirmation
                                                if (newPassword !== confirmPassword) {
                                                    showError('cpasswordError', 'Passwords do not match');
                                                    return false;
                                                }
                                                    return true;
                                                }

                                              })
                                                    // Function to show error message
                                                    function showError(elementId, message) {
                                                        const errorElement = document.getElementById(elementId);
                                                        errorElement.textContent = message;
                                                    }

                                                    // Function to reset error messages
                                                    function resetErrorMessages() {
                                                        const errorElements = document.querySelectorAll('.error-message');
                                                        errorElements.forEach((element) => {
                                                            element.textContent = '';
                                                        });
                                                    }

                                            });
                                          </script>                                          
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
