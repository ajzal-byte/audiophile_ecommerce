        <section class="pt-50 pb-100">
            <div class="container">
                <div class="row">
                    <div class="invoice-link text-lg-end">
                        <h6><a class="text-dark" href="/order-invoice/<%= orderDetails._id %>">Get Invoice</a></h6>
                    </div>
                    <div class="card-body">
                        
                      <div class="table-responsive">
                          <table class="table mb-20">
                              <thead>
                                  <tr>
                                      <th>Product</th>
                                      <th>Date</th>
                                      <th>OrderStatus</th>
                                      <th>PaymentStatus</th>
                                      <th>PaymentMethod</th>
                                      <th>Total</th>
                                  </tr>
                              </thead>
                              <tbody>
                                  <% if (!orderDetails || orderDetails.length === 0) { %>
                                      <h6>No orders</h6>
                                  <% } else { %>
                                      <% orderDetails.products.forEach((item) => { %>
                                        
                                  <tr>
                                      <td> <a href="/productDetails/<%= item.productId._id %>"><%= item.productId.productName %></a> </td>
                                      <td><%= moment(orderDetails.createdAt).format("MMM Do YY") %></td>
                                      <td><%= orderDetails.orderStatus %></td>
                                      <td><%= orderDetails.paymentStatus %></td>
                                      <td><%= orderDetails.paymentMethod %></td>
                                    <% if (item.productId.offerStatus == "Active" &&  item.productId.startDate <= Date.now() && item.productId.endDate > Date.now()) { %>
                                        <% let discountPrice = item.productId.salePrice * item.productId.discountPercentage / 100 * item.quantity; %>
                                        <td>₹<%= item.price * item.quantity - discountPrice %> for <%= item.quantity%> item</td>
                                    <% } else { %>
                                        <td>₹<%= item.price * item.quantity %> for <%= item.quantity%> item</td>
                                    <% } %>
                                  </tr>
                                  
                                  <% }); %>
                              <% } %>
                              </tbody>
                          </table>
                          <h5 class="d-flex justify-content-end mb-10">Subtotal: ₹ <%= orderDetails.totalAmount %></h5>
                          <% if (orderDetails.couponDiscount != 0) { %>
                            <h5 class="d-flex justify-content-end mb-10">Coupon Discount: ₹ <%= orderDetails.couponDiscount %></h5>
                          <% } %>
                          <h5 class="d-flex justify-content-end">Total Paid: ₹ <%= orderDetails.totalAmount - orderDetails.couponDiscount %></h5>
                          <div class="d-flex justify-content-center">
                            <div class="d-flex justify-content-between col-12 align-items-center mt-20">
                              
                                <% if(orderDetails.orderStatus === "Order Placed" || orderDetails.orderStatus === "Shipped") {%> 
                                  <a href="#" id="cancel-button" class="btn btn-sm" data-action="/cancel-order/<%= orderDetails._id %>" style="background-color: rgb(180, 36, 36);">Cancel Order</a>
                                  <%} else if(orderDetails.orderStatus === "Delivered") {%> 
                                    <% if (expiryDate.getTime()  > Date.now()) { %>
                                    <a id="return-button" class="btn btn-sm" style="background-color: rgb(180, 36, 36);">Return Order</a>
                                    <% }else{ %>
                                        <h5 style="color: rgb(177, 29, 29);">The return period is over</h5>
                                    <% } %>
                                    <%} else if(orderDetails.orderStatus === "Cancelled") {%> 
                                      <h5 style="color: rgb(177, 29, 29);">The order is cancelled</h5>
                                      <%} else if (orderDetails.orderStatus === "Returned") {%> 
                                        <h5 style="color: rgb(177, 29, 29);">The order is returned</h5>
                                        <%}%>
                                        <a href="/" class="btn btn-sm">Continue Shopping</a> 
                                    </div>
                                </div>  
                                
                        <script>
                            document.addEventListener("DOMContentLoaded", () => {
                              const cancelButton = document.getElementById('cancel-button');
                          
                                cancelButton.addEventListener('click', (e) => {
                                  e.preventDefault();
                          
                                  const action = cancelButton.getAttribute('data-action');
                          
                                  Swal.fire({
                                    title: "Are you sure?",
                                    text: "You will not be able to revert this!",
                                    icon: "warning",
                                    showCancelButton: true,
                                    confirmButtonColor: "#3085d6",
                                    cancelButtonColor: "#d33",
                                    confirmButtonText: "Yes!"
                                  }).then((result) => {
                                    if (result.isConfirmed) {
                                      window.location.href = action;
                                    }
                                  });
                                });
                            });

                            document.addEventListener("DOMContentLoaded", ()=>{
                                const returnButton = document.getElementById('return-button');
                                returnButton.addEventListener("click", (e)=>{
                                    e.preventDefault();
                                
                                const action = returnButton.getAttribute('data-action');
                                
                                Swal.fire({
                                    title: 'Return Order',
                                    input: 'select',
                                    inputOptions: {
                                        'dislike': "Didn't like it",
                                        'priceDifference': 'Price Decreased',
                                        'defective': 'Defective Product'
                                    },
                                    inputPlaceholder: 'Select a reason',
                                    showCancelButton: true,
                                    confirmButtonColor: '#d33',
                                    cancelButtonColor: '#3085d6',
                                    confirmButtonText: 'Return Order'
                                }).then((result) => {
                                if (result.isConfirmed) {
                                  const selectedReason = result.value;
                                  if(selectedReason){
                                    window.location.href = `/return-order/<%= orderDetails._id %>?returnReason=${selectedReason}`
                                  }
                                }
                              });
                            });
                            })
                          </script>

                      </div>
                  </div>
                </div>
            </div>
        </section>
