                </header>
                <div class="card-body">
                    <div class="table-responsive">
                        <div class="table-responsive">
                            <table class="table align-middle table-nowrap mb-0">
                                <thead class="table-light">
                                    <tr>
                                        <th class="align-middle" scope="col">Order ID</th>
                                        <th class="align-middle" scope="col">Username</th>
                                        <th class="align-middle" scope="col">Date</th>
                                        <th class="align-middle" scope="col">Order Status</th>
                                        <th class="align-middle" scope="col">Payment Method</th>
                                        <th class="align-middle" scope="col">Total</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (orders && orders.length > 0) { %>
                                    <% orders.forEach(order => { %>
                                    <tr>
                                        <td><a href="#" class="fw-bold">orderId_<%= order._id.toString().slice(0, 6) %></a> </td>
                                        <td><%= order.userId?.username ?? "User does not exist anymore" %></td>
                                        <td>
                                            <%= moment(order.orderDate).format("MMM Do YY") %>
                                        </td>
                                        <% if(order.orderStatus === 'Cancelled' || order.orderStatus === 'Order Failed'){ %>
                                            <td><span class="badge rounded-pill alert-danger" ><%= order.orderStatus %></span></td>
                                        <% }else if(order.orderStatus === 'Returned'){ %>
                                          <td><span class="badge rounded-pill alert-warning" ><%= order.orderStatus %></span></td>
                                        <% }else if(order.orderStatus === 'Delivered'){ %>
                                          <td><span class="badge rounded-pill alert-success" ><%= order.orderStatus %></span></td>
                                        <% }else{ %>
                                          <td><span class="badge rounded-pill alert-primary" ><%= order.orderStatus %></span></td>
                                        <% } %>
                                        <td><%= order.paymentMethod %></td>
                                        <td>₹ <%= order.totalAmount %></td>
                                    </tr>
                                    <% }) %>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div> <!-- table-responsive end// -->
                </div>
