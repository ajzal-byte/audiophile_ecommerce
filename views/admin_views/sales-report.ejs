            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Sales report</h2>
                </div>
                
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3 d-flex">
                        <div class="col-lg-8 col-md-6 me-auto">
                            <form action="/admin/filter-report">
                                <div class="row">
                                    <div class="col-lg-3">
                                        <label>Start date</label>
                                        <input type="date" name="startDate" id="startDate" class="form-control" />
                                        <div id="startDateError" class="error-message"></div>
                                    </div>
                                    <div class="col-lg-3">
                                        <label>End date</label>
                                        <input type="date" name="endDate" id="endDate" class="form-control" />
                                        <div id="endDateError" class="error-message"></div>
                                    </div>
                                    <div class="col-lg-3">
                                        <br>
                                        <button style="width: 150px;" type="submit" id="filter" class="btn btn-primary"><i class="text-muted material-icons md-tune"></i>Filter</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <!-- <div class="col-lg-2 col-md-6 me-auto"></div> -->
                        <div class="col-lg-4 col-md-6 me-auto mt-20" style="display: flex; justify-content: flex-end;height: 45px;">
                            <a id="createReport" class="btn btn-primary"><i class="text-muted material-icons md-post_add"></i>Create report</a>
                        </div>
                    </div>
                    <button id="clearFilter" onclick="window,location.href = '/admin/sales-report'" class="btn btn-primary mt-10"><i class="text-muted material-icons md-tune"></i>Clear Filter</button>
                </header>

                <script>
                  document.addEventListener("DOMContentLoaded", ()=>{
                    const filterButton = document.getElementById('filter');
                    filterButton.addEventListener("click", (e)=>{
                      if (!validateForm()) {
                        e.preventDefault();
                      }
                    });
                    function validateForm(){
                      resetErrorMessages(); 
                      const startDate = document.getElementById('startDate').value;
                      const endDate = document.getElementById('endDate').value;

                      let isValid = true;

                      if (startDate == "") {
                        showError('startDateError', 'Enter a date');
                        isValid = false;
                      }else if(endDate == ""){
                        showError('endDateError', 'Enter a date');
                        isValid = false;
                      }
                      return isValid;
                    }

                    function showError(elementId, message) {
                    const errorElement = document.getElementById(elementId);
                    errorElement.textContent = message;
                    errorElement.style.color = 'red';
                    }   

                    function resetErrorMessages() { 
                    const errorElements = document.querySelectorAll('.error-message');
                    errorElements.forEach((element) => {
                        element.textContent = '';
                    });
                    }

                  })
                </script>
                <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="sales" >
                            <thead>
                                <tr>
                                    <th>Order Id</th>
                                    <th>Ordered Date</th>
                                    <th>Username</th>
                                    <th>Payment Status</th>
                                    <th>Order Status</th>
                                    <th>Delivered on</th>
                                    <th>Total Amount</th>
                                </tr>
                            </thead>
                            <tbody>
                              <% if(orders && orders.length > 0){ %>
                                <% orders.forEach((order)=>{ %>
                                    <tr>
                                        <td>orderId_<%= order._id.toString().slice(0, 6) %></td>
                                        <td><%= moment(order.orderDate).format("MMM Do YY") %></td>
                                        <td><%= order.userId?.username ?? "User does not exist anymore" %></td>
                                        <td><%= order.paymentStatus %></td>
                                        <% if(order.orderStatus === 'Cancelled' || order.orderStatus === 'Order Failed'){ %>
                                            <td><span class="badge rounded-pill alert-danger" ><%= order.orderStatus %></span></td>
                                        <% }else if(order.orderStatus === 'Returned'){ %>
                                          <td><span class="badge rounded-pill alert-warning" ><%= order.orderStatus %></span></td>
                                        <% }else if(order.orderStatus === 'Delivered'){ %>
                                          <td><span class="badge rounded-pill alert-success" ><%= order.orderStatus %></span></td>
                                        <% }else{ %>
                                          <td><span class="badge rounded-pill alert-primary" ><%= order.orderStatus %></span></td>
                                        <% } %>
                                        
                                        <td><%= order.deliveryDate? moment(order.deliveryDate).format("MMM Do YY") : 'Not delivered' %></td>
                                        <td>₹ <%= order.totalAmount %></td>
                                    </tr>
                                <% }) %>
                                <% } %>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive//end -->
                </div>
                <!-- card-body end// -->
            </div>
            <!-- Add a new section for sales summary -->
            <div class="card-footer sales-summary">
                <h3>Sales Report Summary</h3>
                <p class="summary-info">This year, we've successfully processed <%= count %> orders.</p>
                <p class="summary-info">Our total revenue reached ₹ <%= totalRevenue %> </p>
            </div>
