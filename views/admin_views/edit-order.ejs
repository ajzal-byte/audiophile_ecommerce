        <section class="mt-50 mb-50">   
          <div class="container">
            <div class="row">
              <div class="col-12">
                  
                <div class="tab-pane " id="orders" role="tabpanel" aria-labelledby="orders-tab">
                  <div class="card">
                      <div class="card-header">
                          <h5 class="mb-0">Your Orders</h5>
                      </div>
                      <div class="card-body">
                          <div class="table-responsive">
                              <table class="table">
                                  <thead>
                                      <tr>
                                          <th>Product</th>
                                          <th>Date</th>
                                          <th>OrderStatus</th>
                                          <th>PaymentStatus</th>
                                          <th>PaymentMethod</th>
                                          <th>Total</th>
                                      </tr>
                                  </thead>
                                  <tbody>
                                      <% if (!orderDetails || orderDetails.length === 0) { %>
                                          <h6>No orders</h6>
                                      <% } else { %>
                                          <% orderDetails.products.forEach((item) => { %>
                                            
                                      <tr>
                                          <td><%= item.productId.productName %></td>
                                          <td><%= orderDetails.createdAt.toLocaleDateString() %></td>
                                          <td><%= orderDetails.orderStatus %></td>
                                          <td><%= orderDetails.paymentStatus %></td>
                                          <td><%= orderDetails.paymentMethod %></td>
                                          <% if (item?.productId?.offerStatus == "Active" &&  item?.productId?.startDate <= Date.now() && item?.productId?.endDate > Date.now()) { %>
                                            <% let discountPrice = item?.productId?.salePrice * item?.productId?.discountPercentage / 100 * item?.quantity; %>
                                            <td>₹<%= item?.price * item?.quantity - discountPrice %> for <%= item?.quantity%> item</td>
                                        <% } else { %>
                                            <td>₹<%= item?.price * item?.quantity %> for <%= item?.quantity%> item</td>
                                        <% } %>
                                      </tr>
                                      
                                      <% }); %>
                                      
                                  <% } %>
                                  </tbody>
                              </table>
                              <h5 class="d-flex justify-content-end">Subtotal: ₹ <%= orderDetails.totalAmount %></h5>
                              <% if (orderDetails.couponDiscount != 0) { %>
                                <h5 class="d-flex justify-content-end mb-10">Coupon Discount: ₹ <%= orderDetails.couponDiscount %></h5>
                              <% } %>
                              <h5 class="d-flex justify-content-end">Total Paid: ₹ <%= orderDetails.totalAmount - orderDetails.couponDiscount %></h5>
                              <div class="d-flex justify-content-center">
                                  
                                <div class="d-flex justify-content-between col-3">
                                 
                                    <% if(orderDetails.orderStatus === "Order Placed" ) {%> 
                                      <a  data-action="/admin/dispatch-order/<%= orderDetails._id %>" id="dispatchButton" class="btn btn-warning btn-sm" >Dispatch product</a>
                                      <a  data-action="/admin/cancel-order/<%= orderDetails._id %>" id="cancelButton" class="btn btn-danger btn-sm cancelButton" >Cancel product</a>
                                      <%} else if(orderDetails.orderStatus === "Shipped") {%> 
                                        <a  data-action="/admin/deliver-order/<%= orderDetails._id %>" id="deliveryButton" class="btn btn-success btn-sm" >Deliver product</a>
                                        <a data-action="/admin/cancel-order/<%= orderDetails._id %>" id="cancelButton2" class="btn btn-danger btn-sm cancelButton" >Cancel product</a>
                                        <%} else if(orderDetails.orderStatus === "Delivered") {%>
                                          <h5 >The order is Delivered</h5>
                                          <%} else if(orderDetails.orderStatus === "Cancelled") {%> 
                                              <div class="text-center"><h5 style="color: rgb(202, 32, 32);">The order is Cancelled</h5></div>
                                          <%} %>
                                </div>

                                <script>
                                   document.addEventListener("DOMContentLoaded", () => {
                                    const dispatchButton = document.getElementById('dispatchButton');
                                    const cancelButton = document.getElementById('cancelButton');
                                    const cancelButton2 = document.getElementById('cancelButton2');
                                    const deliveryButton = document.getElementById('deliveryButton');
                                    if(dispatchButton){
                                        dispatchButton.addEventListener('click', (e) => {
                                            handleButtonClick(e, dispatchButton);
                                        });
                                    }

                                    if (cancelButton) {
                                        cancelButton.addEventListener('click', (e) => {
                                            handleButtonClick(e, cancelButton);
                                        });
                                    }

                                    if (cancelButton2) {
                                        cancelButton2.addEventListener('click', (e) => {
                                            handleButtonClick(e, cancelButton2);
                                        });
                                    }
                                    
                                    if(deliveryButton){
                                        deliveryButton.addEventListener('click', (e) => {
                                            handleButtonClick(e, deliveryButton);
                                        });
                                    }
                                    });

                                    function handleButtonClick(event, button) {
                                    event.preventDefault();
                                    console.log(`${button.id} clicked`);
                                    const action = button.getAttribute('data-action');
                                    console.log('Action:', action);
                                    Swal.fire({
                                        title: "Are you sure?",
                                        text: "You will not be able to revert this!",
                                        icon: "warning",
                                        showCancelButton: true,
                                        confirmButtonColor: "#3085d6",
                                        cancelButtonColor: "#d33",
                                        confirmButtonText: "Yes!"
                                    }).then((result) => {
                                        if (result.isConfirmed) {
                                        window.location.href = action;
                                        }
                                    });
                                    }

                            
                                </script>


                            </div>  
                          </div>
                      </div>
                  </div>
