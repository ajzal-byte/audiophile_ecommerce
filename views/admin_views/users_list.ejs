        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">User Management</h2>
                    <p>View and block users</p>
                </div>
            </div>
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-9">
                            <div class="table-responsive">
                                <table class="table table-hover" id="category">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Email</th>
                                            <th>Phone</th>
                                            <th>Status</th>
                                            <th class=>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% users.reverse().forEach(user => { %>
                                        <tr> 
                                            <td id="userID" >user_<%= user._id.toString().slice(0, 8) %></td>
                                            <td><%= user.email %></td>
                                            <td><%= user.phoneNumber %></td>
                                            <td><%= user.status %></td>
                                            <td> <% if(user.status === "Active") { %>
                                                <a class="btn btn-danger mx-3 block-unblock-button" data-action="/admin/blockUsers/<%= user._id %>" >Block</a>
                                                <% } else { %>
                                                <a class="btn btn-success mx-3 block-unblock-button" data-action="/admin/unblockUsers/<%= user._id %>" >Unblock</a> 
                                                <% } %>
                                            </td>
                                        </tr>
                                        <% }) %>    
                                    </tbody>
                                </table>
                            </div>
                        </div> <!-- .col// -->
                    </div> <!-- .row // -->
                </div> <!-- card body .// -->
